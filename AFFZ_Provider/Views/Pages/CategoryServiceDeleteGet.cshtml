@model AFFZ_Provider.Models.M_SericeDocumentListBinding

<h2>Delete Service Document Binding</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Service Document Binding</h4>
    <dl class="row">
        <dt class="col-sm-2">Category ID</dt>
        <dd class="col-sm-10">@Model.CategoryID</dd>
        <dt class="col-sm-2">Service Document List ID</dt>
        <dd class="col-sm-10">@Model.ServiceDocumentListId</dd>
    </dl>
</div>

<form method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-page="Index" class="btn btn-secondary">Cancel</a>
</form>
